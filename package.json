{
  "name": "nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "posts": "node scripts/getPosts.js",
    "pages": "node scripts/getPages.js",
    "sitemap": "node scripts/sitemap-generator.js",
    "prebuild": "yarn run posts && yarn run pages && yarn run sitemap && next build",
    "build": "next build",
    "export": "next export",
    "prestart": "next build",
    "start": "next start",
    "lint": "next lint",
    "deploy": "yarn run build && next export && touch out/.nojekyll && git add -f out/ && git checkout -b temp-for-deploy-gh-pages && git commit -m \"Deploy Next.js to gh-pages\" && git subtree split --prefix out -b gh-pages && git push -f origin gh-pages:gh-pages && git branch -D gh-pages && git checkout main && git branch -D temp-for-deploy-gh-pages",
    "deploy:window": "yarn run build && next export && fsutil file createnew out/.nojekyll 0 && git add -f out/ && git checkout -b temp-for-deploy-gh-pages && git commit -m \"Deploy Next.js to gh-pages\" && git subtree split --prefix out -b gh-pages && git push -f origin gh-pages:gh-pages && git branch -D gh-pages && git checkout main && git branch -D temp-for-deploy-gh-pages"
  },
  "dependencies": {
    "@craco/craco": "^6.4.3",
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.3.0",
    "@material-ui/core": "^4.12.3",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "axios": "^0.21.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "next": "11.0.1",
    "qs": "^6.10.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-markdown": "4.0.1",
    "react-query": "^3.21.1",
    "react-spring": "^9.4.3",
    "react-syntax-highlighter": "15.4.4",
    "recoil": "^0.4.1",
    "sass": "^1.35.2",
    "utterances-react-component": "^1.0.0"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.3.0",
    "@emotion/eslint-plugin": "^11.2.0",
    "@types/lodash": "^4.14.172",
    "@types/qs": "^6.9.7",
    "@types/react": "17.0.14",
    "@types/react-syntax-highlighter": "^13.5.2",
    "@types/styled-jsx": "^2.2.9",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "craco-alias": "^3.0.1",
    "eslint": "^7.30.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-next": "11.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.3.2",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-prettier": "^1.2.0",
    "stylelint-scss": "^3.19.0",
    "typescript": "4.3.5"
  }
}
